  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:bundle": "bundlewatch",
    "test:a11y": "vitest run src/test/accessibility",
    "test:performance": "vitest run src/test/performance",
    "test:integration": "vitest run src/test/integration",
    "test:all": "npm run test:coverage && npm run test:bundle && npm run test:a11y",
    "analyze": "ANALYZE=true vite build",
    "analyze:bundle": "vite-bundle-analyzer",
    "analyze:treemap": "npm run build && npx rollup-plugin-visualizer dist/bundle-analysis.html --open",
    "analyze:sizes": "npm run build",
    "optimize:bundle": "npm run analyze && npm run test:bundle",
    "electron": "electron .",
    "electron-dev": "concurrently \"pnpm dev\" \"wait-on http://localhost:5173 && pnpm electron\"",
    "build-electron": "node -e \"const fs = require('fs'); if (fs.existsSync('dist-electron')) fs.rmSync('dist-electron', { recursive: true, force: true });\" && pnpm build && electron-builder",
    "build-win": "node -e \"const fs = require('fs'); if (fs.existsSync('dist-electron')) fs.rmSync('dist-electron', { recursive: true, force: true });\" && pnpm build && electron-builder --win",
    "dist": "node -e \"const fs = require('fs'); if (fs.existsSync('dist-electron')) fs.rmSync('dist-electron', { recursive: true, force: true });\" && pnpm build && electron-builder --win --publish=never",
    "clean-dist": "if (Test-Path dist-electron) { Remove-Item -Recurse -Force dist-electron } else { Write-Host 'dist-electron not found' }"
  }
